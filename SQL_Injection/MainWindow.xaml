                <Grid Background="AliceBlue">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="31*"/>
                        <ColumnDefinition Width="101*"/>
                        <ColumnDefinition Width="85*"/>
                        <ColumnDefinition Width="13*"/>
